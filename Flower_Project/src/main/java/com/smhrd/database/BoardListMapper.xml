<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        <mapper namespace="com.smhrd.domain.BoardListDAO">
        	<select id="selecAlltBoard" resultType="com.smhrd.domain.BoardListVO">
        		select * from boardlist
        	</select>
        	<select id="searchBoard" resultType="com.smhrd.domain.BoardListVO">
        		select * from boardlist where b_title == %search%
        	</select>
        </mapper>